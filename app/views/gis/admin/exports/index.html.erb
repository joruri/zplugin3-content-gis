<p class="form">データをCSV形式でエクスポート</p>
<%= form_for(:item, :url => {:action => :index}, :html => {:method => :get, :multipart => true}) do |f| %>
<%= hidden_field_tag :do, 'export' %>
<table class="show">
  <tr>
    <th>データベース</th>
    <td><%= f.select :db_id, @content.form_dbs.map {|t| [t.title, t.id] }, include_blank: true %></td>
  </tr>
</table>

<div class="submitter">
  <%= submit :export, 'エクスポート', id: :export %>
</div>

<% end %>
<%= javascript_tag do %>
$(function() {
  $("*[name=commit_export]").removeAttr('data-disable-with');
});
<% end %>